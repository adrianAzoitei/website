  #knife_00000092455863740721511520000002765213462122515876_ {
    animation: float 2s infinite, pulseAnimation 2s infinite;
  }

  #knife_00000152256179231048915200000006126009471448512696_ {
    animation: float 1s infinite;
    animation-delay: 300ms;
  }

  #knife {
    animation: float 2.5s infinite;
    animation-delay: 500ms;
  }

  #knife_00000023998355766305886940000016486999302549936269_ {
    animation: float 1.5s infinite;
    animation-delay: 300ms;
  }

  .clickable:hover {
    cursor: pointer;
    stroke-width: 0.5rem;
    stroke:white;
  }

  @keyframes pulseAnimation {
    0% {
      stroke-width: 0rem;
      stroke:white;
    }
    50% {
      stroke-width: 0.5rem;
      stroke:white;
    }
    100% {
      stroke-width: 0rem;
      stroke:white;
    }
  }

  @keyframes float {
    0% {
      /* box-shadow: 0 5px 15px 0px rgba(0,0,0,0.6); */
      transform: translatey(0px) rotate(-0.5deg);
    }
    50% {
      /* box-shadow: 0 25px 15px 0px rgba(0,0,0,0.2); */
      transform: translatey(-4px) rotate(0.5deg);
    }
    100% {
      /* box-shadow: 0 5px 15px 0px rgba(0,0,0,0.6); */
      transform: translatey(0px) rotate(-0.5deg);
    }
  }

  @keyframes rotate {
    0% {
      transform: rotate(0deg);
    }
    50% {
      transform: rotate(1deg);
    }
    100% {
    }
  }
</style>

