---
import BaseLayout from "../layouts/BaseLayout.astro";
import HorizontalCard from "../components/HorizontalCard.astro";
---
<style>
  svg {
    overflow: visible;
  }
  g:not(#base):hover {
    stroke-width: 0.5rem;
    stroke:white;
  }
  .pulse {
    width: 100px;
    height: 100px;
    background-color: #3498db;
    border-radius: 50%;
    animation: pulseAnimation 1.5s infinite;
  }
  @keyframes pulseAnimation {
    0% {
      stroke-width: 0rem;
      stroke:white;
    }
    50% {
      stroke-width: 0.5rem;
      stroke:white;
    }
    100% {
      stroke-width: 0rem;
      stroke:white;
    }
  }
</style>

<script>
  // Find all svg elements with the `web` class on the page.
  const cicd = document.querySelectorAll('#cicd');

  // Handle clicks on each button.
  cicd.forEach(button => {
    button.addEventListener('click', () => {
      alert("test");
    });
  })
    
</script>

